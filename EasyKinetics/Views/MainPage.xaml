<!-- MAIN PAGE DESIGN MODULE
     This module designs:
       *) the interface area of all the work environments:
          - SIMPLE ENZYME KINETICS
          - INHIBITION KINETICS
          - ENZYMATIC UNITS ASSAY
          - CALCULATION OF DAbs/min
          - BRADFORD ASSAY
        *) the button area to switch among the work environments
        *) the error messages pop-up
-->
<Page
    x:Class="EasyKinetics.Views.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Style="{StaticResource PageStyle}"
    mc:Ignorable="d" Background="#FFCFCFCF" Width="1280">

    <Grid
        x:Name="ContentArea" Margin="0,0,0,0">
        <Grid                                                     Margin="0,0,0,0"                                                                          Width="1280"    Height="720"                                                                                                                                        Background="PowderBlue"                                                                                                                                                                                         HorizontalAlignment="Stretch"   VerticalAlignment="Stretch">
            <!-- TITLE AREA-->
            <Image          x:Name="Image_logo"                   Margin="450,5,0,0"                                                                        Width="80"      Height="80"                                                                                                                                                                                                                                                                                                         Source="/Assets/StoreLogo.scale-125.png"        HorizontalAlignment="Left"      VerticalAlignment="Top"         Grid.Column="1"/>
            <TextBlock      x:Name="TextBlock_logo"               Margin="550,5,130,0"                  Text="EasyKinetics"                                                 Height="80"                                                         FontSize="72" FontWeight="Bold"     FontStyle="Italic"  FontFamily="AR DELANEY"                             Foreground="Red"                                                                                                                                                                    HorizontalAlignment="Stretch"   VerticalAlignment="Top"/>
            <!-- END OF TITLE AREA-->

            <!-- BUTTON AREA-->
            <Rectangle                                            Margin="10,160,0,0"                                                                       Width="120"     Height="60"     RadiusX="20"    RadiusY="21"                        StrokeThickness="2" Stroke="Black"                                              Fill="#FF56E414"                                                                                                                                                                                                HorizontalAlignment="Left"      VerticalAlignment="Top"/>
            <Button         x:Name="Simple_Enzyme_Kinetics"       Margin="10,160,0,0"                   Content="Simple Enzyme&#xD;&#xA;     Kinetics"      Width="120"     Height="60"                                     Padding="5,4,5,4"   FontSize="12" FontWeight="Bold"                         FontFamily="Verdana"    Background="{x:Null}"                                   BorderThickness="2,2,2,2" BorderBrush="{x:Null}"                                                        Click="SEK_Click"   Loaded="SEK_Loaded"                                         VerticalAlignment="Top"         Opacity="1"/>
            <Rectangle                                            Margin="10,250,0,0"                                                                       Width="120"     Height="60"     RadiusX="20"    RadiusY="21"                        StrokeThickness="2" Stroke="Black"                                              Fill="#FF56E414"                                                                                                                                                                                                HorizontalAlignment="Left"      VerticalAlignment="Top"/>
            <Button         x:Name="Inhibition_Kinetics"          Margin="9,250,0,0"                    Content="Inhibition&#xD;&#xA; Kinetics"             Width="120"     Height="60"                                     Padding="5,4,5,4"   FontSize="12" FontWeight="Bold"                         FontFamily="Verdana"    Background="{x:Null}"                                   BorderThickness="2,2,2,2" BorderBrush="{x:Null}"                                                        Click="IK_Click"    Loaded="IK_Loaded"          HorizontalAlignment="Left"      VerticalAlignment="Top"         Opacity="1"/>
            <Rectangle                                            Margin="10,340,0,0"                                                                       Width="120"     Height="60"     RadiusX="20"    RadiusY="21"                        StrokeThickness="2" Stroke="Black"                                              Fill="#FF56E414"                                                                                                                                                                                                HorizontalAlignment="Left"      VerticalAlignment="Top"/>
            <Button         x:Name="Enzymatic_Units_Assay"        Margin="10,340,0,0"                   Content=" Enzymatic&#xD;&#xA;Units Assay"           Width="120"     Height="60"                                     Padding="5,4,5,4"   FontSize="12" FontWeight="Bold"                         FontFamily="Verdana"    Background="{x:Null}"                                   BorderThickness="2,2,2,2" BorderBrush="{x:Null}"                                                        Click="EUA_Click"   Loaded="EUA_Loaded"                                         VerticalAlignment="Top"         Opacity="1"/>
            <Rectangle                                            Margin="10,430,0,0"                                                                       Width="120"     Height="60"     RadiusX="20"    RadiusY="21"                        StrokeThickness="2" Stroke="Black"                                              Fill="#FF56E414"                                                                                                                                                                                                HorizontalAlignment="Left"      VerticalAlignment="Top"/>
            <Button         x:Name="Calculation_of_dAbs_min"      Margin="10,430,0,0"                   Content=" Calculation&#xD;&#xA;of &#916; Abs/min"   Width="120"     Height="60"                                     Padding="5,4,5,4"   FontSize="12" FontWeight="Bold"                         FontFamily="Verdana"    Background="{x:Null}"                                   BorderThickness="2,2,2,2" BorderBrush="{x:Null}"                                                        Click="dAM_Click"   Loaded="dAM_Loaded"                                         VerticalAlignment="Top"         Opacity="1"/>
            <Rectangle                                            Margin="10,520,0,0"                                                                       Width="120"     Height="60"     RadiusX="20"    RadiusY="21"                        StrokeThickness="2" Stroke="Black"                                              Fill="#FF56E414"                                                                                                                                                                                                HorizontalAlignment="Left"      VerticalAlignment="Top"/>
            <Button         x:Name="Bradford_Assay"               Margin="10,520,0,0"                   Content="Bradford&#xD;&#xA;   Assay"                Width="120"     Height="60"                                     Padding="5,4,5,4"   FontSize="12" FontWeight="Bold"                         FontFamily="Verdana"    Background="{x:Null}"                                   BorderThickness="2,2,2,2" BorderBrush="{x:Null}"                                                        Click="BA_Click"    Loaded="BA_Loaded"                                          VerticalAlignment="Top"         Opacity="1"/>
            <!-- END OF BUTTON AREA-->

            <!-- SIMPLE ENZYME KINETICS AREA -->
            <Canvas         x:Name="cEK1"                         Margin="150,95,0,0"                                                                       Width="1100"    Height="600"                                                        Visibility="Collapsed"                                                          Background="White"                                                                                                                                                                                              HorizontalAlignment="Left"      VerticalAlignment="Top">

                <!-- SIMPLE ENZYME KINETICS AREA: HEADING -->
                <TextBlock  x:Name="EK1_title_1"                  Canvas.Left="250"   Canvas.Top="10"                                                       Width="500"     Height="30"     MinWidth="500"  MinHeight="30"  Padding="5,2,5,2"   FontSize="16" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Center">
                    <Run    Text="SIMPLE ENZYME KINETICS (enzymes with one limiting substrate)"/>
                </TextBlock>
                <TextBlock  x:Name="EK1_title_2"                  Canvas.Left="20"    Canvas.Top="50"   TextWrapping="Wrap"                                 Width="1050"    Height="60"                                     Padding="2,2,2,2"   FontSize="12"                                                                                                                                                                                                                                   PointerPressed="Appropriate_Pressed"            HorizontalTextAlignment="Left">
                    <Run    Text="Prepare 4 samples containing an equal (and "/>
                    <Run    Text="appropriate"                                                                                                              Foreground="Blue" TextDecorations="Underline"/>
                    <Run    Text=") volume of the starting enzyme solution, then put in 2 samples a chosen substrate concentration of [X1] &#956; M and in the last 2 samples a higher substrate concentration of [X2] &#956; M. Finally report the &#916; Abs/min of the samples exposed to the specific wavelength of the reporter product."/>
                        <FlyoutBase.AttachedFlyout >
                            <Flyout>
                                <TextBlock Text="*for example, if the product of the reaction is a chromogen" FontSize="12" FontStyle="Italic"/>
                            </Flyout>
                        </FlyoutBase.AttachedFlyout>
                </TextBlock>
                <!-- SIMPLE ENZYME KINETICS AREA: END OF HEADING -->

                <!-- SIMPLE ENZYME KINETICS AREA: REACTION ANALYSIS FIELDS -->
                <TextBlock  x:Name="EK1_MolecularWeight_ptxt"     Canvas.Left="750"   Canvas.Top="100" Text="Product Molecular Weight"                      Width="165"     Height="24"     MinWidth="12"   MinHeight="24"  Padding="5,2,5,2"   FontSize="12" FontWeight="Bold"                                                    HorizontalTextAlignment="Left"/>
                <TextBox    x:Name="EK1_MolecularWeight"          Canvas.Left="945"   Canvas.Top="100" Text="0"                                             Width="80"      Height="24"     MinWidth="50"   MinHeight="24"  Padding="5,2,5,2"   FontSize="12" FontWeight="Normal"   Background="White" BorderBrush="Black"        IsTabStop="True"  IsTextPredictionEnabled="False" InputScope="Number"                  TextChanged="EK1_MolecularWeight_TextChanged"/>
                <TextBlock  x:Name="EK1_MolecularWeight_stxt"     Canvas.Left="1025"  Canvas.Top="100" Text="g/mol"                                         Width="40"      Height="24"     MinWidth="12"   MinHeight="24"  Padding="5,2,5,2"   FontSize="12" FontWeight="Bold"                                                    HorizontalTextAlignment="Left"/>

                <TextBlock  x:Name="EK1_x1_ptxt"                  Canvas.Left="0"     Canvas.Top="0"    Text="x1"                                           Width="0"       Height="0"      MinWidth="0"    MinHeight="0"   Padding="5,2,5,2"   FontSize="12"                                                                                                                                                                                                                                                                                   HorizontalTextAlignment="Left"/>
                <TextBox    x:Name="EK1_x1"                       Canvas.Left="20"    Canvas.Top="0"    Text="0"                                            Width="0"       Height="0"      MinWidth="0"    MinHeight="0"   Padding="5,2,5,2"   FontSize="12"                                                                   Background="LightPink"                                  IsReadOnly="True"   IsTabStop="False"   InputScope="Number"     IsTextPredictionEnabled="False"         TextChanged="EK1_x1_TextChanged"/>
                <TextBlock  x:Name="EK1_x2_ptxt"                  Canvas.Left="120"   Canvas.Top="0"    Text="x2"                                           Width="0"       Height="0"      MinWidth="0"    MinHeight="0"   Padding="5,2,5,2"   FontSize="12"                                                                                                                                                                                                                                                                                   HorizontalTextAlignment="Left"/>
                <TextBox    x:Name="EK1_x2"                       Canvas.Left="140"   Canvas.Top="0"    Text="0"                                            Width="0"       Height="0"      MinWidth="0"    MinHeight="0"   Padding="5,2,5,2"   FontSize="12"                       Background="LightPink"                                                                              IsReadOnly="True"   IsTabStop="False"   InputScope="Number"     IsTextPredictionEnabled="False"         TextChanged="EK1_x2_TextChanged"/>

                <TextBlock  x:Name="EK1_lSC_ptxt"                 Canvas.Left="0"     Canvas.Top="145"                                                      Width="275"     Height="24"     MinWidth="200"  MinHeight="24"  Padding="5,2,5,2"   FontSize="12" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Left">
                    <Run    Text="Set lower substrate concentration of samples"/>
                </TextBlock>
                <TextBox    x:Name="EK1_lSC"                      Canvas.Left="280"   Canvas.Top="145"  Text="0"                                            Width="80"      Height="24"     MinWidth="50"   MinHeight="24"  Padding="5,2,5,2"   FontSize="12"                       Background="White" BorderBrush="Black"                                                                                  IsTabStop="True"    InputScope="Number"     IsTextPredictionEnabled="False"         TextChanged="EK1_lSC_TextChanged"               LostFocus="EK1_lSC_LostFocus"/>
                <TextBlock  x:Name="EK2_lSC_stxt"                 Canvas.Left="360"   Canvas.Top="145"  Text="&#956; M"                                     Width="35"      Height="24"     MinWidth="20"   MinHeight="24"  Padding="2,2,2,2"   FontSize="12" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Left"/>

                <TextBlock  x:Name="EK1_lSC_dAbsmin1_ptxt"        Canvas.Left="10"    Canvas.Top="170"  Text="1st sample"                                   Width="70"      Height="24"     MinWidth="50"   MinHeight="24"  Padding="2,2,5,2"   FontSize="12" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Left"/>
                <TextBox    x:Name="EK1_lSC_dAbsmin1"             Canvas.Left="85"    Canvas.Top="170"  Text="0"                                            Width="80"      Height="24"     MinWidth="50"   MinHeight="24"  Padding="5,2,5,2"   FontSize="12"                       Background="White" BorderBrush="Black"                                                                                  IsTabStop="True"    InputScope="Number"     IsTextPredictionEnabled="False"         TextChanged="EK1_lSC_dAbsmin1_TextChanged"      LostFocus="EK1_lSC_LostFocus"/>
                <TextBlock  x:Name="EK1_lSC_dAbsmin1_stxt"        Canvas.Left="165"   Canvas.Top="170"  Text="&#916; Abs/min"                               Width="70"      Height="24"     MinWidth="50"   MinHeight="24"  Padding="5,2,5,2"   FontSize="12" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Left"/>

                <TextBlock  x:Name="EK1_lSC_dAbsmin2_ptxt"        Canvas.Left="10"    Canvas.Top="200"  Text="2nd sample"                                   Width="70"      Height="24"     MinWidth="50"   MinHeight="24"  Padding="2,2,5,2"   FontSize="12" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Left"/>
                <TextBox    x:Name="EK1_lSC_dAbsmin2"             Canvas.Left="85"    Canvas.Top="200"  Text="0"                                            Width="80"      Height="24"     MinWidth="50"   MinHeight="24"  Padding="5,2,5,2"   FontSize="12"                       Background="White" BorderBrush="Black"                                                                                  IsTabStop="True"    InputScope="Number"     IsTextPredictionEnabled="False"         TextChanged="EK1_lSC_dAbsmin2_TextChanged"      LostFocus="EK1_lSC_LostFocus"/>
                <TextBlock  x:Name="EK1_lSC_dAbsmin2_stxt"        Canvas.Left="165"   Canvas.Top="200"  Text="&#916; Abs/min"                               Width="70"      Height="24"     MinWidth="50"   MinHeight="24"  Padding="5,2,5,2"   FontSize="12" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Left"/>

                <TextBlock  x:Name="EK1_lSC_dAbsmin_ptxt"         Canvas.Left="260"   Canvas.Top="170"  Text="atot"                                         Width="0"       Height="0"      MinWidth="0"    MinHeight="0"   Padding="2,2,2,2"   FontSize="12"                                                                                                                                                                                                                                                                                   HorizontalTextAlignment="Left"/>
                <TextBox    x:Name="EK1_lSC_dAbsmin"              Canvas.Left="300"   Canvas.Top="170"  Text="0"                                            Width="0"       Height="0"      MinWidth="0"    MinHeight="0"   Padding="5,2,5,2"   FontSize="12"                       Background="LightPink"                                                                              IsReadOnly="True"   IsTabStop="False"   InputScope="Number"     IsTextPredictionEnabled="False"         TextChanged="EK1_lSC_dAbsmin_TextChanged"/>

                <TextBlock  x:Name="EK1_y2_ptxt"                  Canvas.Left="260"   Canvas.Top="200"  Text="y2"                                           Width="0"       Height="0"      MinWidth="0"    MinHeight="0"   Padding="5,2,5,2"   FontSize="12"                                                                                                                                                                                                                                                                                   HorizontalTextAlignment="Left"/>
                <TextBox    x:Name="EK1_y2"                       Canvas.Left="300"   Canvas.Top="200"  Text="N/A"                                          Width="0"       Height="0"      MinWidth="0"    MinHeight="0"   Padding="5,2,5,2"   FontSize="12"                       Background="LightPink"                                                                              IsReadOnly="True"   IsTabStop="False"   InputScope="Number"     IsTextPredictionEnabled="False"         TextChanged="EK1_y2_TextChanged"/>

                <TextBlock  x:Name="EK1_hSC_ptxt"                 Canvas.Left="0"     Canvas.Top="235"                                                      Width="280"     Height="24"     MinWidth="200"  MinHeight="24"  Padding="5,2,5,2"   FontSize="12" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Left">
                    <Run    Text="Set higher substrate concentration of samples"/>
                </TextBlock>
                <TextBox    x:Name="EK1_hSC"                      Canvas.Left="280"   Canvas.Top="235"  Text="0"                                            Width="80"      Height="24"     MinWidth="50"   MinHeight="24"  Padding="5,2,5,2"   FontSize="12"                       Background="White" BorderBrush="Black"                                                                                  IsTabStop="True"    InputScope="Number"     IsTextPredictionEnabled="False"         TextChanged="EK1_hSC_TextChanged"               LostFocus="EK1_hSC_LostFocus"/>
                <TextBlock  x:Name="EK1_hSC_stxt"                 Canvas.Left="360"   Canvas.Top="235"  Text="&#956; M"                                     Width="35"      Height="24"     MinWidth="20"   MinHeight="24"  Padding="2,2,2,2"   FontSize="12" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Left"/>

                <TextBlock  x:Name="EK1_hSC_dAbsmin1_ptxt"        Canvas.Left="10"    Canvas.Top="260"  Text="1st sample"                                   Width="70"      Height="24"     MinWidth="50"   MinHeight="24"  Padding="2,2,5,2"   FontSize="12" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Left"/>
                <TextBox    x:Name="EK1_hSC_dAbsmin1"             Canvas.Left="85"    Canvas.Top="260"  Text="0"                                            Width="80"      Height="24"     MinWidth="50"   MinHeight="24"  Padding="5,2,5,2"   FontSize="12"                       Background="White" BorderBrush="Black"                                                                                  IsTabStop="True"    InputScope="Number"     IsTextPredictionEnabled="False"         TextChanged="EK1_hSC_dAbsmin1_TextChanged"      LostFocus="EK1_hSC_LostFocus"/>
                <TextBlock  x:Name="EK1_hSC_dAbsmin1_stxt"        Canvas.Left="165"   Canvas.Top="260"  Text="&#916; Abs/min"                               Width="70"      Height="24"     MinWidth="50"   MinHeight="24"  Padding="5,2,5,2"   FontSize="12" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Left"/>

                <TextBlock  x:Name="EK1_hSC_dAbsmin2_ptxt"        Canvas.Left="10"    Canvas.Top="290"  Text="2nd sample"                                   Width="70"      Height="24"     MinWidth="50"   MinHeight="24"  Padding="2,2,5,2"   FontSize="12" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Left"/>
                <TextBox    x:Name="EK1_hSC_dAbsmin2"             Canvas.Left="85"    Canvas.Top="290"  Text="0"                                            Width="80"      Height="24"     MinWidth="50"   MinHeight="24"  Padding="5,2,5,2"   FontSize="12"                       Background="White" BorderBrush="Black"                                                                                  IsTabStop="True"    InputScope="Number"     IsTextPredictionEnabled="False"         TextChanged="EK1_hSC_dAbsmin2_TextChanged"      LostFocus="EK1_hSC_LostFocus"/>
                <TextBlock  x:Name="EK1_hSC_dAbsmin2_stxt"        Canvas.Left="165"   Canvas.Top="290"  Text="&#916; Abs/min"                               Width="70"      Height="24"     MinWidth="50"   MinHeight="24"  Padding="5,2,5,2"   FontSize="12" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Left"/>

                <TextBlock  x:Name="EK1_hSC_dAbsmin_ptxt"         Canvas.Left="260"   Canvas.Top="260"  Text="btot"                                         Width="0"       Height="0"      MinWidth="0"    MinHeight="0"   Padding="5,2,5,2"   FontSize="12"                                                                                                                                                                                                                                                                                   HorizontalTextAlignment="Left"/>
                <TextBox    x:Name="EK1_hSC_dAbsmin"              Canvas.Left="300"   Canvas.Top="260"  Text="0"                                            Width="0"       Height="0"      MinWidth="0"    MinHeight="0"   Padding="5,2,5,2"   FontSize="12"                       Background="LightPink"                                                                              IsReadOnly="True"   IsTabStop="False"   InputScope="Number"     IsTextPredictionEnabled="False"         TextChanged="EK1_hSC_dAbsmin_TextChanged"/>

                <TextBlock  x:Name="EK1_y1_ptxt"                  Canvas.Left="260"   Canvas.Top="290"  Text="y1"                                           Width="0"       Height="0"      MinWidth="0"    MinHeight="0"   Padding="5,2,5,2"   FontSize="12"                                                                                                                                                                                                                                                                                   HorizontalTextAlignment="Left"/>
                <TextBox    x:Name="EK1_y1"                       Canvas.Left="300"   Canvas.Top="290"  Text="N/A"                                          Width="0"       Height="0"      MinWidth="0"    MinHeight="0"   Padding="5,2,5,2"   FontSize="12"                       Background="LightPink"                                                                              IsReadOnly="True"   IsTabStop="False"   InputScope="Number"     IsTextPredictionEnabled="False"         TextChanged="EK1_y1_TextChanged"/>

                <TextBlock  x:Name="EK1_tgAngolo_ptxt"            Canvas.Left="225"   Canvas.Top="340" Text="tgAng"                                         Width="0"       Height="0"      MinWidth="0"    MinHeight="0"   Padding="2,2,5,2"   FontSize="12"                                                                                                                                                                                                                                                                                   HorizontalTextAlignment="Left"/>
                <TextBox    x:Name="EK1_tgAngolo"                 Canvas.Left="265"   Canvas.Top="340" Text="N/A"                                           Width="0"       Height="0"      MinWidth="0"    MinHeight="0"   Padding="5,2,5,2"   FontSize="12"                       Background="LightPink"                                                                              IsReadOnly="True"   IsTabStop="False"   InputScope="Number"     IsTextPredictionEnabled="False"         TextChanged="EK1_tgAngolo_TextChanged"/>
                <!-- SIMPLE ENZYME KINETICS AREA: END OF REACTION ANALYSIS FIELDS -->

                <!-- SIMPLE ENZYME KINETICS AREA: SUBSTRATE INHIBITION ANALYSIS FIELDS -->
                <TextBlock  x:Name="EK1_SubstrInhibTest_ptxt"     Canvas.Left="0"     Canvas.Top="320"  TextWrapping="Wrap"                                 Width="260"     Height="24"     MinWidth="50"   MinHeight="12"  Padding="5,2,2,2"   FontSize="12" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Left">
                    <Run    Text="Set a sample with substrate concentration of"/>
                </TextBlock>
                <TextBox    x:Name="EK1_Absorb_SubstrConc"        Canvas.Left="265"   Canvas.Top="320"  Text="0"                                            Width="80"      Height="24"     MinWidth="50"   MinHeight="24"  Padding="5,2,5,2"   FontSize="12"                       Background="#66E3F907"                                                                              IsReadOnly="True"   IsTabStop="True"    InputScope="Number"     IsTextPredictionEnabled="False"         TextChanged="EK1_Absorb_SubstrConc_TextChanged" GotFocus="EK1_Absorb_SubstrConc_GotFocus"/>
                <TextBlock  x:Name="EK1_Absorb_SubstrConc_stxt"   Canvas.Left="342"   Canvas.Top="320"  Text="&#956; M"                                     Width="30"      Height="24"     MinWidth="10"   MinHeight="24"  Padding="5,2,5,2"   FontSize="12" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Left"/>
                <TextBlock  x:Name="EK1_Absorbance_ptxt"          Canvas.Left="10"    Canvas.Top="350"  Text="Absorbance change per minute"                 Width="180"     Height="24"     MinWidth="50"   MinHeight="12"  Padding="5,2,5,2"   FontSize="12" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Left"/>
                <TextBox    x:Name="EK1_Absorbance"               Canvas.Left="200"   Canvas.Top="350"  Text="0"                                            Width="80"      Height="24"     MinWidth="50"   MinHeight="12"  Padding="5,2,5,2"   FontSize="12"                       Background="White" BorderBrush="Black"                                                                                  IsTabStop="True"    InputScope="Number"     IsTextPredictionEnabled="False"         TextChanged="EK1_Absorbance_TextChanged"/>
                <TextBlock  x:Name="EK1_Absorbance_stxt"          Canvas.Left="280"   Canvas.Top="350"  Text="&#916; Abs/min"                               Width="70"      Height="24"     MinWidth="50"   MinHeight="12"  Padding="5,2,5,2"   FontSize="12" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Left"/>
                <TextBlock  x:Name="EK1_Absorb_Flag_ptxt"         Canvas.Left="10"    Canvas.Top="380"  Text="Substrate-Inhibition test flag"               Width="0"       Height="0"      MinWidth="0"    MinHeight="0"   Padding="5,2,5,2"   FontSize="12" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Left"/>
                <TextBox    x:Name="EK1_Absorb_Flag"              Canvas.Left="200"   Canvas.Top="380"  Text="0"                                            Width="0"       Height="0"      MinWidth="0"    MinHeight="0"   Padding="5,2,5,2"   FontSize="12"                       Background="#66E3F907"                                                                              IsReadOnly="True"   IsTabStop="True"    InputScope="Number"     IsTextPredictionEnabled="False"         TextChanged="EK1_Absorb_Flag_TextChanged"/>
                <TextBlock  x:Name="EK1_Absorb_Result_ptxt"       Canvas.Left="10"    Canvas.Top="380"  Text="Substrate-Inhibition test result"             Width="0"       Height="0"      MinWidth="0"    MinHeight="0"   Padding="5,2,5,2"   FontSize="12" FontWeight="Bold"                                                                                                                                                                                                                                                                 HorizontalTextAlignment="Left"/>
